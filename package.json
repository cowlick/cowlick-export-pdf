{
  "name": "@cowlick/export-pdf",
  "version": "0.0.1",
  "description": "pdf exporter for cowlick scenario",
  "main": "lib/index.js",
  "bin": {
    "cowlick-export-pdf": "./bin/run"
  },
  "files": [
    "bin",
    "fonts",
    "lib"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:src && npm run build:scenario && npm run scan:assets && npm run build:pdf",
    "build:src": "tsc -p .",
    "build:scenario": "cowlick-kag-compiler ./examples/scenario.ks -o ./script",
    "build:pdf": "node ./lib/cli.js scenario",
    "scan:assets": "akashic scan asset"
  },
  "author": "pocketberserker",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cowlick/cowlick-export-pdf.git"
  },
  "dependencies": {
    "@cowlick/core": "0.8.0",
    "commandpost": "1.3.0",
    "pdfmake": "0.1.37"
  },
  "devDependencies": {
    "@akashic/akashic-cli": "1.3.2",
    "@akashic/akashic-engine": "2.3.7",
    "@cowlick/kag-compiler": "0.8.0",
    "@types/node": "9.4.7",
    "@types/pdfmake": "0.1.1",
    "typescript": "3.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
